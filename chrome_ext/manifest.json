{
	"name": "Diepio Script Loader",
	"version": "1.0",
	"manifest_version": 2,
	"description": "Load a custom d.js script for Diep.io",
	"icons": {  "16": "icons/icon-16.png",
				"32": "icons/icon-32.png",
				"48": "icons/icon-48.png",
				"64": "icons/icon-64.png",
				"128": "icons/icon-128.png" },
	"browser_action": {
		"default_icon": "icons/icon-128.png",
		"default_title": "Diepio Script Loader"
	},
	"permissions": ["webRequest", "webRequestBlocking", "*://*.diep.io/*", "tabs"],
	"background": {
		"scripts": ["background.js"]
	}	
}